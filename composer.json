{
    "name": "skydiver/blade-icons-remix",
    "description": "Use Remix Icon with Blade Icons.",
    "keywords": ["Laravel", "Blade", "Icons", "Remix"],
    "homepage": "https://github.com/skydiver/blade-icons-remix",
    "license": "MIT",
    "authors": [
        {
            "name": "Mart√≠n M.",
            "email": "skydiver@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "blade-ui-kit/blade-icons": "^0.5.0",
        "illuminate/support": "^7.14|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Skydiver\\BladeIconsRemix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs --standard=PSR2 -v ./src",
        "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 -w -v ./src"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skydiver\\BladeIconsRemix\\BladeIconsRemixServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
